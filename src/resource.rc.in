#include "resource.h"

MAIN_ICON ICON RES_ICON_ICO_PATH

LANGUAGE LANG_NEUTRAL, SUBLANG_NEUTRAL
1 VERSIONINFO
    FILEVERSION     @PROJECT_VERSION_MAJOR@,@PROJECT_VERSION_MINOR@,0,0
    PRODUCTVERSION  @PROJECT_VERSION_MAJOR@,@PROJECT_VERSION_MINOR@,0,0
    FILEOS          VOS_UNKNOWN
    FILETYPE        VFT_UNKNOWN
    FILESUBTYPE     VFT2_UNKNOWN
    FILEFLAGSMASK   VS_FF_SPECIALBUILD
    FILEFLAGS       VS_FF_PRIVATEBUILD
{
    BLOCK "StringFileInfo"
    {
        BLOCK "080AFDE9"
        {
            VALUE "Comments", "Welcome to the crossplatform world"
            VALUE "CompanyName", "@AUTHOR@"
			VALUE "FileDescription", "gtk3 crossplatform application"
            VALUE "FileVersion", "@PROJECT_VERSION_MAJOR@,@PROJECT_VERSION_MINOR@"
            VALUE "InternalName", "@PROJECT_NAME@"
			VALUE "LegalCopyright", "@EMAIL@"
            VALUE "ProductVersion", "@PROJECT_VERSION@"
        }
    }
    BLOCK "VarFileInfo"
    {
        VALUE "Translation", 0x080A, 0xFDE9
    }
}
